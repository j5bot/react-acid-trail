{
  "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest/globals": true
  },
  "extends": [
    "j5bot",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
  },
  "plugins": [
      "jest",
      "react"
  ]
}
